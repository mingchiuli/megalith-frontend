    "axios": "^1.6.8",
    "element-plus": "^2.6.2",
    "highlight.js": "^11.9.0",
    "js-base64": "^3.7.7",
    "markdown-it": "^14.1.0",
    "md-editor-v3": "^4.12.2",
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node18": "^2.0.1",
